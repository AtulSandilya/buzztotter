{
    "compilerOptions": {
        "target": "es5",
        "allowJs": true,
        "jsx": "react",
        "outDir": "adminBuild",
        "rootDir": "src",
        "sourceMap": false,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "lib": ["ES2015", "dom", "es5"]
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
      "components",
      "containers",
      "reducers",
      "routers",
      "sagas",
      "app.tsx",
      "configureStore.ts",
      "Utilities.ts"
    ]
}

