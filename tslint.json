{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "no-magic-numbers": true,
    "no-switch-case-fall-through": true,
    "switch-default": false,
    "jsx-no-multiline-js": false,
    "arrow-parens": false,
    "jsx-no-lambda": false,
    "jsx-wrap-multiline": false,
    "jsx-alignment": false
  }
}
